# Raptor Suite Environment Variables
# Copy this file to .env.local for local development

# Firebase Config (Get from Firebase Console)
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=raptor-suite.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=raptor-suite
VITE_FIREBASE_STORAGE_BUCKET=raptor-suite.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id

# AI API Keys (Store in Firebase Secret Manager for production)
# These are only needed for local development
OPENAI_API_KEY=your-openai-key
ANTHROPIC_API_KEY=your-anthropic-key
GEMINI_API_KEY=your-gemini-key
FIGMA_API_KEY=your-figma-key